{
	"name": "swipers",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://Calanthe.github.io/swipers/",
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"classnames": "^2.2.6",
		"husky": "^4.3.0",
		"lint-staged": "^10.4.0",
		"node-sass": "^7.0.0",
		"prettier": "^2.1.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-easy-swipe": "0.0.21",
		"react-redux": "^7.1.3",
		"react-scripts": "3.3.0",
		"redux": "^4.0.4",
		"redux-thunk": "^2.3.0",
		"typescript": "^3.7.3"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write"
		]
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"deploy": "gh-pages -d build",
		"format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"prettier": {
		"arrowParens": "always",
		"semi": true,
		"useTabs": true
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"gh-pages": "^2.2.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
